{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@workshop-nx/static/booking/feature-shell": [
        "components/static/booking/feature-shell/src/index.ts"
      ],
      "@workshop-nx/static/booking/data-access": [
        "components/static/booking/data-access/src/index.ts"
      ],
      "@workshop-nx/static/check-in/feature-shell": [
        "components/static/check-in/feature-shell/src/index.ts"
      ],
      "@workshop-nx/static/check-in/data-access": [
        "components/static/check-in/data-access/src/index.ts"
      ],
      "@workshop-nx/static/shared/data-access": [
        "components/static/shared/data-access/src/index.ts"
      ],
      "@workshop-nx/static/shared/seatmap/feature-seatmap": [
        "components/static/shared/seatmap/feature-seatmap/src/index.ts"
      ],
      "@workshop-nx/static/shared/seatmap/data-access": [
        "components/static/shared/seatmap/data-access/src/index.ts"
      ],
      "@workshop/core/entities": ["components/core/entities/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
